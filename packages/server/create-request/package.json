{
  "name": "@modern-js/create-request",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.0.0",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "browser": "./dist/js/treeshaking/browser.js",
  "files": [
    "dist",
    "hook.d.ts"
  ],
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/node.js",
        "require": "./dist/js/node/node.js"
      },
      "default": "./dist/js/treeshaking/browser.js"
    },
    "./client": "./dist/js/treeshaking/browser.js",
    "./modern": "./dist/js/modern/browser.js",
    "./server": "./dist/js/node/node.js",
    "./hook": "./dist/js/node/hook.js"
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/runtime": "workspace:^1.0.0",
    "@modern-js/utils": "workspace:^1.0.0",
    "node-fetch": "^2.6.1",
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "typescript": "^4",
    "@modern-js/plugin-testing": "^1.0.0",
    "@modern-js/module-tools": "^1.0.0",
    "@modern-js/plugin-ssr": "workspace:^1.0.0"
  },
  "peerDependencies": {
    "@modern-js/plugin-ssr": "workspace:^1.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
