{
  "name": "prod-server-pure",
  "version": "0.1.0",
  "scripts": {
    "dev": "modern dev",
    "build": "wireit",
    "start": "modern start",
    "new": "modern new",
    "lint": "modern lint",
    "deploy": "modern deploy",
    "test": "wireit"
  },
  "dependencies": {},
  "devDependencies": {},
  "modernConfig": {
    "runtime": {
      "router": true,
      "state": true
    }
  },
  "wireit": {
    "build": {
      "command": "modern build",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json"
      ],
      "output": [
        "dist/**/*"
      ]
    },
    "test": {
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json"
      ],
      "output": []
    }
  }
}
