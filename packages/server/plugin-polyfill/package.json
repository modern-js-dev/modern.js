{
  "name": "@modern-js/plugin-polyfill",
  "version": "1.0.0-rc.9",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./cli": {
      "node": {
        "import": "./dist/js/node/cli.js",
        "require": "./dist/js/node/cli.js"
      },
      "default": "./dist/js/node/cli.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "dev": "modern build --watch",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/core": "workspace:^1.0.0-rc.9",
    "@modern-js/polyfill-lib": "^1.0.0-rc.0",
    "@modern-js/server-plugin": "workspace:^1.0.0-rc.9",
    "lru-cache": "^6.0.0",
    "mime-types": "^2.1.32",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@modern-js/types": "workspace:^1.0.0-rc.9",
    "@types/jest": "^26",
    "@types/lru-cache": "^5.1.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/ua-parser-js": "^0.7.36",
    "typescript": "^4",
    "@modern-js/plugin-testing": "^1.0.0-rc.9",
    "@modern-js/module-tools": "^1.0.0-rc.9"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
