{
  "name": "@modern-js/generator-utils",
  "description": "Modern.js generator common utils, include create project and new option",
  "version": "1.0.0-rc.14",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/plugin-i18n": "workspace:^1.0.0-rc.14",
    "@modern-js/utils": "workspace:^1.0.0-rc.14",
    "execa": "^5.1.1",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@modern-js/codesmith": "^1.0.0-rc.16",
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4",
    "@modern-js/module-tools": "^1.0.0-rc.14",
    "@modern-js/plugin-testing": "^1.0.0-rc.14"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
