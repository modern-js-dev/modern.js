{
  "name": "@modern-js/generator-plugin-plugin",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.0.0",
  "jsnext:source": "./src/index.ts",
  "main": "./dist/js/node/main.js",
  "files": [
    "/templates",
    "/dist/js/node/main.js"
  ],
  "meta": {
    "key": "generator-plugin",
    "name": "生成器插件",
    "type": "module"
  },
  "scripts": {
    "prepare": "pnpm build && pnpm build:csmith",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "build:csmith": "csmith-tools build",
    "test": "modern test --passWithNoTests"
  },
  "devDependencies": {
    "@babel/runtime": "^7",
    "@modern-js/generator-common": "workspace: ^1.0.0",
    "@modern-js/generator-plugin": "workspace: ^1.0.0",
    "@modern-js/plugin-i18n": "workspace: ^1.0.0",
    "@types/jest": "^26",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14",
    "typescript": "^4",
    "@modern-js/module-tools": "^1.1.0",
    "@modern-js/plugin-testing": "^1.1.0",
    "@modern-js/codesmith-tools": "^1.0.0"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js",
      "disableSourceMap": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://bnpm.byted.org/"
  }
}
