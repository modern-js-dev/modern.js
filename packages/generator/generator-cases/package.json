{
  "name": "@modern-js/generator-cases",
  "description": "Modern.js all generator cases, includes create project and new option",
  "version": "1.0.0-rc.19",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "execa": "^5.1.1",
    "covertable": "^2.2.2",
    "@modern-js/generator-common": "workspace:1.0.0-rc.20"
  },
  "devDependencies": {
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4",
    "@modern-js/module-tools": "^1.0.0-rc.19",
    "@modern-js/plugin-testing": "^1.0.0-rc.19"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
