{
  "name": "@modern-js/monorepo-generator",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.3.3",
  "jsnext:source": "./src/index.ts",
  "main": "./dist/js/node/index.js",
  "files": [
    "/templates",
    "/dist/js/node/main.js"
  ],
  "scripts": {
    "prepare": "pnpm build && pnpm build:csmith",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "build:csmith": "csmith-tools build",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@babel/runtime": "^7",
    "@modern-js/base-generator": "workspace:^1.3.2",
    "@modern-js/changeset-generator": "workspace:^1.2.2",
    "@modern-js/plugin-i18n": "workspace:^1.2.1",
    "@modern-js/codesmith": "^1.0.8",
    "@modern-js/codesmith-api-app": "^1.0.8",
    "@modern-js/codesmith-tools": "^1.0.9",
    "@modern-js/codesmith-api-json": "^1.0.7",
    "@modern-js/generator-common": "workspace:^1.4.3",
    "@modern-js/generator-utils": "workspace:^1.2.1",
    "@scripts/build": "workspace:*",
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4",
    "jest": "^27",
    "@scripts/jest-config": "workspace:*"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js",
      "disableSourceMap": true
    }
  },
  "publishConfig": {
    "main": "./dist/js/node/main.js",
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "types": "./src/index.ts"
}
