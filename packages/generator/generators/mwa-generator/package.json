{
  "name": "@modern-js/mwa-generator",
  "description": "Modern.js create mwa project generator",
  "version": "1.0.0-rc.16",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/main.js",
  "files": [
    "/templates",
    "/dist/js/node/main.js"
  ],
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build && pnpm build:csmith",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "build:csmith": "csmith-tools build",
    "test": "modern test --passWithNoTests"
  },
  "devDependencies": {
    "@babel/runtime": "^7",
    "@modern-js/base-generator": "workspace:^1.0.0-rc.16",
    "@modern-js/codesmith": "^1.0.0-rc.16",
    "@modern-js/codesmith-api-app": "^1.0.0-rc.16",
    "@modern-js/codesmith-api-json": "^1.0.0-rc.16",
    "@modern-js/codesmith-tools": "^1.0.0-rc.16",
    "@modern-js/entry-generator": "workspace:^1.0.0-rc.16",
    "@modern-js/electron-generator": "workspace:^1.0.0-rc.17",
    "@modern-js/dependence-generator": "workspace:^1.0.0-rc.16",
    "@modern-js/generator-common": "workspace:^1.0.0-rc.17",
    "@modern-js/generator-utils": "workspace:^1.0.0-rc.16",
    "@modern-js/plugin-i18n": "workspace:^1.0.0-rc.16",
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4",
    "@modern-js/module-tools": "^1.0.0-rc.16",
    "@modern-js/plugin-testing": "^1.0.0-rc.16"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js",
      "disableSourceMap": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
