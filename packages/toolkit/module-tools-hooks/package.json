{
  "name": "@modern-js/module-tools-hooks",
  "version": "1.0.0-rc.8",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/core": "workspace:^1.0.0-rc.8",
    "@modern-js/plugin": "workspace:^1.0.0-rc.8"
  },
  "devDependencies": {
    "@modern-js/style-compiler": "workspace:^1.0.0-rc.8",
    "typescript": "^4.4.3",
    "@modern-js/plugin-testing": "^1.0.0-rc.8",
    "@modern-js/module-tools": "^1.0.0-rc.8"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
