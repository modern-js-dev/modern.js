{
  "name": "@modern-js/esmpack",
  "version": "1.2.2",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "jsnext:source": "./src/index.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.js"
      },
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "del-cli ./dist && tsc -p tsconfig.build.json",
    "build:watch": "tsc -p ./tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "serve:fixture": "serve -l 8088 fixture",
    "test": "jest --passWithNoTests",
    "test:e2e": "JEST_PUPPETEER_CONFIG=e2e/jest-puppeteer.config.js jest -c ./e2e/jest.config.js --run-in-band",
    "test:unit": "jest -c ./__tests__/jest.config.js"
  },
  "dependencies": {
    "@modern-js/utils": "workspace:^1.3.3",
    "@babel/core": "7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@babel/traverse": "~7.12.12",
    "@babel/types": "~7.12.12",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-inject": "~4.0.2",
    "@rollup/plugin-json": "~4.1.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@rollup/plugin-replace": "~2.3.4",
    "@rollup/plugin-wasm": "~5.1.1",
    "@rollup/plugin-url": "~6.1.0",
    "chalk": "~4.1.0",
    "enhanced-resolve": "^5.8.3",
    "es-module-lexer": "~0.3.26",
    "find-up": "~5.0.0",
    "fs-extra": "~9.0.1",
    "is-builtin-module": "^3.0.0",
    "less": "~4.1.0",
    "lodash": "~4.17.20",
    "postcss": "~8.2.4",
    "resolve": "~1.19.0",
    "rollup": "~2.50.6",
    "rollup-plugin-node-polyfills": "~0.2.1",
    "@svgr/rollup": "~6.2.0",
    "sass": "~1.32.2",
    "tapable": "~2.2.0",
    "validate-npm-package-name": "~3.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "~7.1.12",
    "@types/babel__traverse": "~7.14.2",
    "@types/es-module-lexer": "~0.3.0",
    "@types/fs-extra": "~9.0.6",
    "@types/gunzip-maybe": "~1.4.0",
    "@types/jest": "~26.0.20",
    "@types/jest-environment-puppeteer": "~4.4.1",
    "@types/less": "~3.0.2",
    "@types/lodash": "~4.14.167",
    "@types/node": "~14.14.20",
    "@types/puppeteer": "~5.4.2",
    "@types/resolve": "~1.17.1",
    "@types/sass": "~1.16.0",
    "@types/semver": "^7.3.4",
    "@types/strip-comments": "~2.0.0",
    "@types/tar-fs": "~2.0.0",
    "@types/validate-npm-package-name": "~3.0.2",
    "axios": "^0.24.0",
    "del-cli": "^4.0.1",
    "execa": "~5.0.0",
    "expect-puppeteer": "~4.4.0",
    "gunzip-maybe": "~1.4.2",
    "jest": "^27.2.1",
    "jest-environment-puppeteer": "~6.0.0",
    "jest-puppeteer": "~6.0.0",
    "puppeteer": "~10.4.0",
    "semver": "^7.3.2",
    "serve": "~11.3.2",
    "strip-comments": "^2.0.1",
    "tar-fs": "~2.1.1",
    "tempy": "~1.0.0",
    "ts-jest": "~27.0.5",
    "typescript": "^4",
    "@scripts/jest-config": "workspace:*"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "types": "./src/index.ts"
}
