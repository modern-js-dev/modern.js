{
  "name": "@modern-js/utils",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.3.7",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "jsnext:modern": "./dist/index.js",
  "exports": {
    ".": {
      "jsnext:source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./format": {
      "jsnext:source": "./src/format.ts",
      "default": "./dist/format.js"
    },
    "./constants": {
      "jsnext:source": "./src/constants.ts",
      "default": "./dist/constants.js"
    },
    "./chalk": {
      "jsnext:source": "./compiled/chalk/index.js",
      "default": "./compiled/chalk/index.js"
    },
    "./strip-ansi": {
      "jsnext:source": "./compiled/strip-ansi/index.js",
      "default": "./compiled/strip-ansi/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "format": [
        "./dist/format.d.ts"
      ],
      "constants": [
        "./dist/constants.d.ts"
      ],
      "chalk": [
        "./compiled/chalk/index.d.ts"
      ],
      "strip-ansi": [
        "./compiled/strip-ansi/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "new": "modern new",
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "typescript": "^4"
  },
  "devDependencies": {
    "@modern-js/types": "workspace:^1.3.6",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/debug": "4.1.7",
    "@types/fs-extra": "9.0.13",
    "@types/glob": "^7.1.4",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/signale": "1.4.4",
    "@types/js-yaml": "4.0.5",
    "@types/recursive-readdir": "2.2.0",
    "jest": "^27",
    "browserslist": "4.20.2",
    "import-lazy": "4.0.0",
    "chalk": "4.1.2",
    "chokidar": "3.5.3",
    "commander": "8.3.0",
    "debug": "4.3.4",
    "glob": "7.2.0",
    "execa": "5.1.1",
    "filesize": "8.0.7",
    "signale": "1.4.0",
    "fs-extra": "10.0.1",
    "strip-ansi": "6.0.1",
    "pkg-up": "3.1.0",
    "recursive-readdir": "2.2.2",
    "gzip-size": "6.0.0",
    "upath": "2.0.1",
    "js-yaml": "4.1.0",
    "webpack": "^5.71.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/index.d.ts"
  }
}
