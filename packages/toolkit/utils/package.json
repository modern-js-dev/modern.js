{
  "name": "@modern-js/utils",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.6.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "jsnext:modern": "./dist/index.js",
  "_comment": "Provide ESM and CJS exports, ESM is used by runtime package, for treeshaking",
  "exports": {
    ".": {
      "jsnext:source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./format": {
      "jsnext:source": "./src/format.ts",
      "default": "./dist/format.js"
    },
    "./constants": {
      "jsnext:source": "./src/constants.ts",
      "default": "./dist/constants.js"
    },
    "./ssr": {
      "jsnext:source": "./src/ssr.ts",
      "default": "./dist/ssr.js"
    },
    "./json5": {
      "jsnext:source": "./compiled/json5/index.js",
      "default": "./compiled/json5/index.js"
    },
    "./lodash": {
      "jsnext:source": "./compiled/lodash/index.js",
      "default": "./compiled/lodash/index.js"
    },
    "./globby": {
      "jsnext:source": "./compiled/globby/index.js",
      "default": "./compiled/globby/index.js"
    },
    "./fast-glob": {
      "jsnext:source": "./compiled/fast-glob/index.js",
      "default": "./compiled/fast-glob/index.js"
    },
    "./strip-ansi": {
      "jsnext:source": "./compiled/strip-ansi/index.js",
      "default": "./compiled/strip-ansi/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "format": [
        "./dist/format.d.ts"
      ],
      "constants": [
        "./dist/constants.d.ts"
      ],
      "ssr": [
        "./dist/ssr.d.ts"
      ],
      "json5": [
        "./compiled/json5/index.d.ts"
      ],
      "lodash": [
        "./compiled/lodash/index.d.ts"
      ],
      "globby": [
        "./compiled/globby/index.d.ts"
      ],
      "fast-glob": [
        "./compiled/fast-glob/index.d.ts"
      ],
      "strip-ansi": [
        "./compiled/strip-ansi/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern new",
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001332",
    "lodash": "^4.17.21",
    "typescript": "^4"
  },
  "devDependencies": {
    "@modern-js/types": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/jest": "^26",
    "@types/node": "^14",
    "jest": "^27",
    "webpack": "^5.71.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/index.d.ts"
  }
}
