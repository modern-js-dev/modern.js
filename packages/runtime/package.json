{
  "name": "@modern-js/runtime-core",
  "version": "1.0.0-rc.16",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./styled": {
      "node": {
        "import": "./dist/js/modern/styled.js",
        "require": "./dist/js/node/styled.js"
      },
      "default": "./dist/js/treeshaking/styled.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "styled": [
        "./dist/types/styled.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/plugin": "workspace:^1.0.0-rc.16",
    "@modern-js/utils": "workspace:^1.0.0-rc.16",
    "@types/styled-components": "^5.1.14",
    "invariant": "^2.2.4",
    "styled-components": "^5.3.1"
  },
  "peerDependencies": {
    "@loadable/component": "^5.15.0",
    "@types/loadable__component": "^5.13.4"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@loadable/component": "^5.15.0",
    "@types/loadable__component": "^5.13.4",
    "@types/invariant": "^2.2.30",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4",
    "@modern-js/plugin-testing": "^1.0.0-rc.16",
    "@modern-js/module-tools": "^1.0.0-rc.16"
  },
  "sideEffects": false
}
