{
  "name": "@modern-js/plugin-ssg",
  "version": "1.0.0-rc.5",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./cli": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "dev": "modern build --watch",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/babel-chain": "workspace:^1.0.0-rc.5",
    "@modern-js/core": "workspace:^1.0.0-rc.5",
    "@modern-js/utils": "workspace:^1.0.0-rc.5",
    "node-mocks-http": "^1.10.1",
    "normalize-path": "^3.0.0",
    "portfinder": "^1.0.28",
    "react-router-dom": "^5.2.1",
    "webpack-chain": "^6.5.1"
  },
  "devDependencies": {
    "@modern-js/server": "workspace:^1.0.0-rc.5",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "typescript": "^4",
    "@modern-js/plugin-testing": "^1.0.0-rc.5",
    "@modern-js/module-tools": "^1.0.0-rc.5"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
