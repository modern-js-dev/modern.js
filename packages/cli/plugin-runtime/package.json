{
  "name": "@modern-js/runtime",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.0.0-rc.22",
  "jsnext:source": "./src/index.ts",
  "types": "./type.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": "./dist/js/node/index.js",
      "default": "./dist/js/treeshaking/index.js"
    },
    "./loadable": {
      "node": "./dist/js/node/exports/loadable.js",
      "default": "./dist/js/treeshaking/exports/loadable.js"
    },
    "./head": {
      "node": "./dist/js/node/exports/head.js",
      "default": "./dist/js/treeshaking/exports/head.js"
    },
    "./styled": {
      "node": "./dist/js/node/exports/styled.js",
      "default": "./dist/js/treeshaking/exports/styled.js"
    },
    "./server": {
      "node": "./dist/js/node/exports/server.js",
      "default": "./dist/js/treeshaking/exports/server.js"
    },
    "./router": {
      "node": "./dist/js/node/exports/router.js",
      "default": "./dist/js/treeshaking/exports/router.js"
    },
    "./ssr": {
      "node": "./dist/js/node/exports/ssr.js",
      "default": "./dist/js/treeshaking/exports/ssr.js"
    },
    "./model": {
      "node": "./dist/js/node/exports/model.js",
      "default": "./dist/js/treeshaking/exports/model.js"
    },
    "./cli": "./dist/js/node/cli/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./type.d.ts"
      ],
      "loadable": [
        "./dist/types/exports/loadable.d.ts"
      ],
      "head": [
        "./dist/types/exports/head.d.ts"
      ],
      "styled": [
        "./dist/types/exports/styled.d.ts"
      ],
      "server": [
        "./dist/types/exports/server.d.ts"
      ],
      "ssr": [
        "./dist/types/exports/ssr.d.ts"
      ],
      "router": [
        "./dist/types/exports/router.d.ts"
      ],
      "model": [
        "./types/model.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@loadable/component": "^5.15.0",
    "@modern-js/bff-runtime": "workspace:^1.0.0-rc.22",
    "@modern-js/plugin-router": "workspace:^1.0.0-rc.22",
    "@modern-js/plugin-ssr": "workspace:^1.0.0-rc.22",
    "@modern-js/plugin-state": "workspace:^1.0.0-rc.22",
    "@modern-js/runtime-core": "workspace:^1.0.0-rc.22",
    "@modern-js/utils": "workspace:^1.0.0-rc.22",
    "@types/loadable__component": "^5.13.4",
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.1.13",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "typescript": "^4",
    "@modern-js/core": "workspace:^1.0.0-rc.22",
    "@modern-js/plugin-testing": "^1.0.0-rc.22",
    "@modern-js/module-tools": "^1.0.0-rc.22"
  },
  "peerDependencies": {
    "@modern-js/core": "workspace:^1.0.0-rc.22"
  },
  "sideEffects": false,
  "modernConfig": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
