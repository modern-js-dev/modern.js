{
  "name": "@modern-js/plugin-less",
  "version": "1.0.0-rc.2",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./cli": "./dist/js/node/cli.js"
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/core": "workspace:^1.0.0-rc.2",
    "@modern-js/css-config": "workspace:^1.0.0-rc.2",
    "@modern-js/utils": "workspace:^1.0.0-rc.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "less-plugin-npm-import": "^2.1.0"
  },
  "devDependencies": {
    "@modern-js/style-compiler": "workspace:^1.0.0-rc.2",
    "@types/jest": "^26",
    "@types/less": "^3.0.3",
    "@types/node": "^14",
    "typescript": "^4",
    "webpack": "^5.54.0",
    "webpack-chain": "^6.5.1",
    "@modern-js/plugin-testing": "^1.0.0-rc.2",
    "@modern-js/module-tools": "^1.0.0-rc.2"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
