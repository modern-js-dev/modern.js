{
  "name": "@modern-js/plugin-design-token",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.0.0",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/runtime/index.d.ts",
  "main": "./dist/js/node/runtime/index.js",
  "module": "./dist/js/treeshaking/runtime/index.js",
  "jsnext:modern": "./dist/js/modern/runtime/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/runtime/index.js",
        "require": "./dist/js/node/runtime/index.js"
      },
      "default": "./dist/js/treeshaking/runtime/index.js"
    },
    "./cli": "./dist/js/node/cli/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/runtime/index.d.ts"
      ],
      "plugins": [
        "./dist/types/plugins.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/utils": "workspace:^1.1.0",
    "tailwindcss": "^2.0.4"
  },
  "devDependencies": {
    "@modern-js/module-tools": "^1.1.0",
    "@modern-js/plugin-testing": "^1.1.0",
    "@modern-js/runtime-core": "workspace:^1.1.0",
    "@modern-js/core": "workspace:^1.1.0",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "react": "^17.0.2",
    "typescript": "^4"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@modern-js/runtime-core": "workspace:^1.1.0",
    "@modern-js/core": "workspace:^1.1.0",
    "react": "^17.0.2"
  },
  "modernConfig": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
