{
  "name": "@modern-js/plugin-testing",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.1.4",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    "./type": "./type.d.ts",
    ".": {
      "node": {
        "import": "./dist/js/modern/runtime-testing/index.js",
        "require": "./dist/js/node/runtime-testing/index.js",
        "types": "./types/index.d.ts"
      },
      "default": "./dist/js/treeshaking/runtime-testing/index.js"
    },
    "./runtime": {
      "node": {
        "import": "./dist/js/modern/runtime-testing/index.js",
        "require": "./dist/js/node/runtime-testing/index.js",
        "types": "./types/index.d.ts"
      },
      "default": "./dist/js/treeshaking/runtime/index.js"
    },
    "./cli": "./dist/js/node/cli/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/runtime/index.d.ts"
      ],
      "cli": [
        "./dist/types/cli/index.d.ts"
      ],
      "runtime": [
        "./dist/types/runtime-testing/index.d.ts"
      ],
      "type": [
        "./type.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7",
    "@modern-js/babel-compiler": "workspace:^1.0.0",
    "@modern-js/testing": "workspace:^1.0.0",
    "@modern-js/utils": "workspace:^1.0.0",
    "@modern-js/webpack": "workspace:^1.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "enhanced-resolve": "^5.8.2",
    "@modern-js/testing-plugin-bff": "workspace:^1.1.1"
  },
  "peerDependencies": {
    "@modern-js-reduck/plugin-auto-actions": "^1.0.0",
    "@modern-js-reduck/plugin-effects": "^1.0.0",
    "@modern-js-reduck/plugin-immutable": "^1.0.0",
    "@modern-js-reduck/store": "^1.0.0",
    "@modern-js/core": "workspace:^1.0.0",
    "@modern-js/runtime-core": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@modern-js-reduck/plugin-auto-actions": "^1.0.0",
    "@modern-js-reduck/plugin-effects": "^1.0.0",
    "@modern-js-reduck/plugin-immutable": "^1.0.0",
    "@modern-js-reduck/store": "^1.0.0",
    "@modern-js/plugin-testing": "^1.1.1",
    "@modern-js/module-tools": "^1.1.1",
    "@modern-js/core": "workspace:^1.0.0",
    "@modern-js/runtime-core": "workspace:^1.0.0",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "typescript": "^4"
  },
  "sideEffects": false,
  "modernConfig": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
