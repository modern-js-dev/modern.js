{
  "name": "@modern-js/plugin-server-build",
  "version": "1.0.0-rc.12",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "dev": "modern build --watch",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/babel-compiler": "workspace:^1.0.0-rc.12",
    "@modern-js/core": "workspace:^1.0.0-rc.12",
    "@modern-js/server-utils": "workspace:^1.0.0-rc.12",
    "@modern-js/utils": "workspace:^1.0.0-rc.12",
    "globby": "^12.0.2",
    "json5": "^2.2.0"
  },
  "devDependencies": {
    "@modern-js/plugin-analyze": "workspace:^1.0.0-rc.12",
    "@types/babel__core": "^7.1.15",
    "@types/jest": "^26",
    "@types/node": "^14",
    "ts-jest": "^27.0.5",
    "typescript": "^4",
    "@modern-js/plugin-testing": "^1.0.0-rc.12",
    "@modern-js/module-tools": "^1.0.0-rc.12"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
