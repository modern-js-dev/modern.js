{
  "name": "@modern-js/plugin-server-build",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.2.1",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "dev": "modern build --watch",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/babel-compiler": "workspace:^1.2.1",
    "@modern-js/server-utils": "workspace:^1.2.1",
    "@modern-js/utils": "workspace:^1.2.2",
    "globby": "^12.0.2",
    "json5": "^2.2.0"
  },
  "devDependencies": {
    "@modern-js/plugin-analyze": "workspace:^1.2.1",
    "@types/babel__core": "^7.1.15",
    "@types/jest": "^26",
    "@types/node": "^14",
    "ts-jest": "^27.0.5",
    "typescript": "^4",
    "@modern-js/core": "workspace:^1.3.2",
    "@scripts/build": "workspace:*",
    "jest": "^27",
    "@scripts/jest-config": "workspace:*"
  },
  "peerDependencies": {
    "@modern-js/core": "workspace:^1.3.2"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts"
  }
}
