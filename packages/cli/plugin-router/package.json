    "@modern-js/core": "workspace:^1.0.0-rc.12",
    "@modern-js/plugin": "workspace:^1.0.0-rc.12",
    "@modern-js/plugin-analyze": "workspace:^1.0.0-rc.12",
    "@modern-js/plugin-testing": "^1.0.0-rc.12",
    "@modern-js/runtime-core": "workspace:^1.0.0-rc.12",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "react": "^17",
    "react-dom": "^17",
    "ts-jest": "^27.0.4",
    "typescript": "^4",
    "@modern-js/module-tools": "^1.0.0-rc.12"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
